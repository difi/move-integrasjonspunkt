<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="http://schemas.altinn.no/services/serviceEntity/2014/10" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://schemas.altinn.no/services/serviceEntity/2014/10">
  <xs:import schemaLocation="CorrespondenceAgencyExternal_2.xsd" namespace="http://schemas.microsoft.com/2003/10/Serialization/"/>
  <xs:simpleType name="CorrespondenceStatusTypeV2">
    <xs:annotation>
      <xs:appinfo/>
      <xs:documentation>&lt;summary>
            Defines the different statuses a correspondence can have through it's lifetime.
            &lt;/summary></xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="NotSet">
        <xs:annotation>
          <xs:documentation>&lt;summary>
            Specifies that the current status of the correspondence isn't important or unknown.
            &lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Created">
        <xs:annotation>
          <xs:documentation>&lt;summary>
            Specifies that the correspondence has been created.
            &lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Read">
        <xs:annotation>
          <xs:documentation>&lt;summary>
            Specifies that the recipient has read the correspondence.
            &lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Confirmed">
        <xs:annotation>
          <xs:appinfo>
            <EnumerationValue xmlns="http://schemas.microsoft.com/2003/10/Serialization/">6</EnumerationValue>
          </xs:appinfo>
          <xs:documentation>&lt;summary>
            Specifies that the recipient has confirmed the correspondence.
            &lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Reserved">
        <xs:annotation>
          <xs:appinfo>
            <EnumerationValue xmlns="http://schemas.microsoft.com/2003/10/Serialization/">10</EnumerationValue>
          </xs:appinfo>
          <xs:documentation>&lt;summary>
            Specifies that correspondence message is reserved as the recipient has opted out for reservation from electronic messages.
            &lt;/summary></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="CorrespondenceStatusTypeV2" nillable="true" type="tns:CorrespondenceStatusTypeV2"/>
</xs:schema>