    </wsdl:operation>
    <wsdl:operation name="InsertCorrespondence">
      <wsdl:documentation><![CDATA[<summary>
            This operation is used to send a correspondence to a user.
            </summary>
            <param name="SystemUserCode">
            System user code is the unique representation of the agency system - mandatory parameter
            </param>
            <param name="ExternalReference">
            An external reference to the batch request - mandatory parameter
            </param>
            <param name="Correspondence">
            InsertCorrespondenceBE entity with information about the correspondence - mandatory parameter
            </param>
            <returns>
            A ReceiptExternalBE entity with reference information for the correspondence
            </returns>]]></wsdl:documentation>
      <wsdl:input wsaw:Action="http://www.altinn.no/services/ServiceEngine/Correspondence/2009/10/ICorrespondenceAgencyExternal/InsertCorrespondence" message="tns:ICorrespondenceAgencyExternal_InsertCorrespondence_InputMessage"/>
      <wsdl:output wsaw:Action="http://www.altinn.no/services/ServiceEngine/Correspondence/2009/10/ICorrespondenceAgencyExternal/InsertCorrespondenceResponse" message="tns:ICorrespondenceAgencyExternal_InsertCorrespondence_OutputMessage"/>
      <wsdl:fault wsaw:Action="http://www.altinn.no/services/ServiceEngine/Correspondence/2009/10/ICorrespondenceAgencyExternal/InsertCorrespondenceAltinnFaultFault" name="AltinnFaultFault" message="tns:ICorrespondenceAgencyExternal_InsertCorrespondence_AltinnFaultFault_FaultMessage"/>
    </wsdl:operation>
    <wsdl:operation name="InsertCorrespondenceV2">
      <wsdl:documentation><![CDATA[<summary>
            This operation is used to send a correspondence to a user.
            </summary>
            <remarks>
            This operation is versioned as V2 as it contain some additional information. Preferred over InsertCorrespondence.
            Optional additional information include CaseId used if the correspondence(s) shall be a part of a collaboration service
            and AllowForwarding in order to specify whether the correspondence(s) has the ability to be forwarded as email or not by
            the end user in SBL. Note that when not submitting the AllowForwarding element it is default that the correspondence(s) can be forwarded.
            </remarks>
            <param name="SystemUserCode">
            System user code is the unique representation of the agency system - mandatory parameter
            </param>
            <param name="ExternalShipmentReference">
            An external reference to the batch request - mandatory parameter
            </param>
            <param name="Correspondence">
            InsertCorrespondenceBEV2 entity with information about the correspondence - mandatory parameter
            </param>
            <returns>
            A ReceiptExternalBE entity with reference information for the correspondence
            </returns>]]></wsdl:documentation>
      <wsdl:input wsaw:Action="http://www.altinn.no/services/ServiceEngine/Correspondence/2009/10/ICorrespondenceAgencyExternal/InsertCorrespondenceV2" message="tns:ICorrespondenceAgencyExternal_InsertCorrespondenceV2_InputMessage"/>
      <wsdl:output wsaw:Action="http://www.altinn.no/services/ServiceEngine/Correspondence/2009/10/ICorrespondenceAgencyExternal/InsertCorrespondenceV2Response" message="tns:ICorrespondenceAgencyExternal_InsertCorrespondenceV2_OutputMessage"/>
      <wsdl:fault wsaw:Action="http://www.altinn.no/services/ServiceEngine/Correspondence/2009/10/ICorrespondenceAgencyExternal/InsertCorrespondenceV2AltinnFaultFault" name="AltinnFaultFault" message="tns:ICorrespondenceAgencyExternal_InsertCorrespondenceV2_AltinnFaultFault_FaultMessage"/>
    </wsdl:operation>
  </wsdl:portType>
